                                    android:background="@drawable/rectangle_bg_white_a700_border_gray_200_radius_12"
                                    android:id="@+id/btnSettings"
                                    android:layout_height="@dimen/_51pxh"
                                    android:layout_marginStart="@dimen/_12pxh"
                                    android:layout_width="@dimen/_51pxh"
                                    android:paddingHorizontal="@dimen/_14pxh"
                                    android:paddingVertical="@dimen/_14pxv"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/img_settings_indigo_a200" tools:ignore="ContentDescription"/>
                            </LinearLayout>
                        </androidx.appcompat.widget.Toolbar>
                        <LinearLayout android:gravity="end"
                            android:id="@+id/linearRowglobe"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_45pxv"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_width="wrap_content"
                            android:orientation="horizontal" style="@style/groupStylecornerRadius_3">
