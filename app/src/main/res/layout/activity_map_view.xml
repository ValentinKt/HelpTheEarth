                    android:id="@+id/horizontalScrollHorizontalscrollgroup18499"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:scrollbars="none">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumngroup18499"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:orientation="vertical" style="@style/groupStyle_2">
