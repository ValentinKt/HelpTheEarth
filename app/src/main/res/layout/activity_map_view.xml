                                    android:drawableStart="@drawable/img_arrowleft_gray_904"
                                    android:ellipsize="end"
                                    android:hint="@string/msg_find_for_food_o"
                                    android:id="@+id/etGroup18499"
                                    android:inputType="text"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.84"
                                    android:layout_width="0dp"
                                    android:paddingBottom="@dimen/_17pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_17pxh"
                                    android:paddingTop="@dimen/_14pxv"
                                    android:singleLine="true"
                                    android:text="@={mapViewVM.mapViewModel.etGroup18499Value}"
