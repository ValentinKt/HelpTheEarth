<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="searchWhiteBarVM" type="com.seeeeshtheearth.app.modules.searchwhitebar.data.viewmodel.SearchWhiteBarVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearSearchWhiteB"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTitleBar2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHelloAshfak"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_weight="0.84"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{searchWhiteBarVM.searchWhiteBarModel.txtHelloAshfak}"
                    style="@style/txtAirbnbCerealAppmedium24" tools:text="@string/lbl_search2"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearRow"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearRowsearch"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.63" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSearch"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search_indigo_a200"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search_indigo_a200"/>
                <View android:background="@color/indigo_300"
                    android:id="@+id/lineLineFiftyFour"
                    android:layout_height="@dimen/_26pxv"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.01" android:layout_width="0dp"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSearchOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_weight="0.83"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{searchWhiteBarVM.searchWhiteBarModel.txtSearchOne}"
                    style="@style/txtAirbnbCerealApplight24" tools:text="@string/lbl_search"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowfilter"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_119pxh"
                android:layout_weight="0.37" android:layout_width="0dp"
                android:orientation="horizontal" style="@style/groupStyleindigo_2">
                <ImageView android:id="@+id/imageFilter"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_filter"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_filter"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFilters"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_8pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_weight="0.63"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{searchWhiteBarVM.searchWhiteBarModel.txtFilters}"
                    style="@style/txtAirbnbCerealAppbook12_3" tools:text="@string/lbl_filters"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearCard1"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_6">
            <FrameLayout android:id="@+id/frameStackOne"
                android:layout_height="@dimen/_92pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_79pxh" style="@style/groupStyleorange_200cornerRadius">
                <ImageView android:id="@+id/imageImageEightyOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_92pxh"
                    android:layout_width="@dimen/_79pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_10pxh}"
                    imageRes="@{@drawable/img_image81}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image81"/>
                <FrameLayout android:id="@+id/frameStack"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_92pxv" android:layout_width="@dimen/_79pxh">
                    <ImageView android:id="@+id/imageImageEightyFive"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_92pxh"
                        android:layout_width="@dimen/_79pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_image85}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image85"/>
                    <ImageView android:id="@+id/imageImageEightySix"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_92pxh"
                        android:layout_width="@dimen/_79pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_image86}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image86"/>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumntime"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_55pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_weight="0.68" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{searchWhiteBarVM.searchWhiteBarModel.txtTime}"
                    android:textAllCaps="true"
                    style="@style/txtAirbnbCerealAppmedium12_1" tools:text="@string/msg_1st_may_sat"/>
                <TextView android:id="@+id/txtImGoingtoSh"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    android:text="@{searchWhiteBarVM.searchWhiteBarModel.txtImGoingtoSh}"
                    style="@style/txtAirbnbCerealAppmedium18_6" tools:text="@string/msg_a_virtual_eveni"/>
            </LinearLayout>
        </LinearLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="center_horizontal"
            android:id="@+id/recyclerListtimeOne"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_17pxv"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_16pxh}" tools:itemCount="4" tools:listitem="@layout/row_listtime_one"/>
    </LinearLayout>
</layout>
