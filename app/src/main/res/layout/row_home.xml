<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeRowModel" type="com.seeeeshtheearth.app.modules.home.data.model.HomeRowModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumngroup33261"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
        <FrameLayout android:id="@+id/frameStackgroup33261"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_131pxv"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_9pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="@dimen/_218pxh" style="@style/groupStyleorange_200cornerRadius">
            <ImageView android:id="@+id/imageGroup33261"
                android:layout_gravity="start"
                android:layout_height="@dimen/_131pxh"
                android:layout_width="@dimen/_218pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group33261"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group33261"/>
            <FrameLayout android:id="@+id/frameStackarmanrokni"
                android:layout_gravity="start"
                android:layout_height="@dimen/_131pxv" android:layout_width="@dimen/_218pxh">
                <ImageView android:id="@+id/imageImageEightyFour"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_131pxh"
                    android:layout_width="@dimen/_218pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_10pxh}"
                    imageRes="@{@drawable/img_image84}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image84"/>
                <LinearLayout android:id="@+id/linearRowarmanrokni"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_8pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnarmanrokni"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.6"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStylewhite_A700_b2cornerRadius">
                        <TextView android:gravity="center_horizontal"
                            android:id="@+id/txtArmanrokni"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginEnd="@dimen/_9pxh"
                            android:layout_marginStart="@dimen/_9pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:text="@{homeRowModel.txtArmanrokni}"
                            android:textAllCaps="true"
                            style="@style/txtAirbnbCerealAppmedium14" tools:text="@string/lbl_10_june"/>
                    </LinearLayout>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_white_a700_b2_radius_7"
                        android:id="@+id/btnMap"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginStart="@dimen/_127pxh"
                        android:layout_width="@dimen/_30pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_map" tools:ignore="ContentDescription"/>
                </LinearLayout>
            </FrameLayout>
        </FrameLayout>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtImGoingtoSh"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{homeRowModel.txtImGoingtoSh}"
            style="@style/txtAirbnbCerealAppmedium18_1" tools:text="@string/msg_international_b"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowoval"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <FrameLayout android:id="@+id/frameStackoval"
                android:layout_height="@dimen/_24pxv" android:layout_width="@dimen/_56pxh">
                <ImageView android:id="@+id/imageOval"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_oval}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_oval"/>
                <ImageView android:id="@+id/imageOvalCopy"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ovalcopy}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ovalcopy"/>
                <ImageView android:id="@+id/imageOvalCopyFour"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ovalcopy4}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ovalcopy4"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txt20Going"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_weight="0.51" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{homeRowModel.txt20Going}"
                style="@style/txtAirbnbCerealAppmedium12" tools:text="@string/lbl_20_going"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowmappin"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageMappin"
                android:layout_height="@dimen/_14pxh"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_12pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_mappin"
                tools:ignore="ContentDescription" tools:src="@drawable/img_mappin"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.93" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{homeRowModel.txtLanguage}"
                style="@style/txtAirbnbCerealAppbook13" tools:text="@string/msg_36_guild_street"/>
        </LinearLayout>
    </LinearLayout>
</layout>
