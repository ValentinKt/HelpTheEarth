<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="organizerProfileReviewVM" type="com.seeeeshtheearth.app.modules.organizerprofilereview.data.viewmodel.OrganizerProfileReviewVM"/>
    </data>
    <androidx.core.widget.NestedScrollView android:clipToPadding="false"
        android:fillViewport="true"
        android:id="@+id/scrollViewScrollview"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnellipsefiftytwo"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListellipsefiftytwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_width="match_parent"
                android:nestedScrollingEnabled="false"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_20pxh}" tools:itemCount="3" tools:listitem="@layout/row_listellipsefiftytwo"/>
            <FrameLayout android:id="@+id/frameStackrectangle4172"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_109pxv"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_324pxh">
                <View
                    android:background="@drawable/rectangle_bg_white_a700_radius_14"
                    android:id="@+id/viewRectangle4172"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_109pxv"
                    android:layout_marginEnd="@dimen/_1pxh" android:layout_width="@dimen/_324pxh"/>
                <LinearLayout android:id="@+id/linearRowrectangle"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_8pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangle"
                        android:layout_height="@dimen/_92pxh"
                        android:layout_width="@dimen/_92pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle_92x92}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_92x92"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumntime"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_weight="0.69"
                        android:layout_width="0dp" android:orientation="vertical">
                        <LinearLayout android:id="@+id/linearRowtime"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_5pxh"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTime"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.9"
                                android:layout_width="0dp"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:singleLine="true"
                                android:text="@{organizerProfileReviewVM.organizerProfileReviewModel.txtTime}"
                                style="@style/txtAirbnbCerealAppmedium13_2" tools:text="@string/msg_10_june_9_00"/>
                            <ImageView android:id="@+id/imageBookmark"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginStart="@dimen/_81pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_width="@dimen/_12pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_bookmark_gray_506"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_bookmark_gray_506"/>
                        </LinearLayout>
                        <TextView android:id="@+id/txtImGoingtoSh"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:text="@{organizerProfileReviewVM.organizerProfileReviewModel.txtImGoingtoSh}"
                            style="@style/txtAirbnbCerealAppbold15" tools:text="@string/msg_international_g2"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowmappin"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageMappin"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_2pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_12pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_mappin_14x12"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_mappin_14x12"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtLanguage"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:layout_weight="0.93"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{organizerProfileReviewVM.organizerProfileReviewModel.txtLanguage}"
                                style="@style/txtAirbnbCerealAppbook13_4" tools:text="@string/msg_36_guild_street2"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</layout>
