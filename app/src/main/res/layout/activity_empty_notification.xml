<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="emptyNotificationVM" type="com.seeeeshtheearth.app.modules.emptynotification.data.viewmodel.EmptyNotificationVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearEmptyNotificat"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearActionBarTit"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_weight="0.99"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHelloAshfak"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{emptyNotificationVM.emptyNotificationModel.txtHelloAshfak}"
                        style="@style/txtAirbnbCerealAppmedium24" tools:text="@string/lbl_notification"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageMorevertical"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_148pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="@dimen/_1pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_morevertical"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_morevertical"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout android:id="@+id/frameArtwork"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_169pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_168pxv" android:layout_width="@dimen/_155pxh">
            <FrameLayout android:id="@+id/frameStackbell"
                android:layout_gravity="end"
                android:layout_height="@dimen/_168pxv"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_136pxh">
                <ImageView android:id="@+id/imageBell"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_168pxh"
                    android:layout_width="@dimen/_136pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_bell"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_bell"/>
                <ImageView android:id="@+id/imageFace"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginHorizontal="@dimen/_40pxh"
                    android:layout_marginVertical="@dimen/_40pxv"
                    android:layout_width="@dimen/_46pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_face"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_face"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtGroup287"
                android:layout_gravity="bottom|end"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:paddingBottom="@dimen/_7pxv"
                android:paddingEnd="@dimen/_11pxh"
                android:paddingStart="@dimen/_11pxh"
                android:paddingTop="@dimen/_6pxv"
                android:singleLine="true"
                android:text="@{emptyNotificationVM.emptyNotificationModel.txtGroup287}"
                style="@style/txtSolidRounded_7" tools:text="@string/lbl_0"/>
            <ImageView android:id="@+id/imageLine"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_26pxh"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_line"
                tools:ignore="ContentDescription" tools:src="@drawable/img_line"/>
            <ImageView android:id="@+id/imageLineOne"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_33pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="@dimen/_21pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_line"
                tools:ignore="ContentDescription" tools:src="@drawable/img_line"/>
        </FrameLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtTitle"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_44pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_6pxh"
            android:singleLine="true"
            android:text="@{emptyNotificationVM.emptyNotificationModel.txtTitle}"
            style="@style/txtAirbnbCerealAppmedium18_8" tools:text="@string/msg_no_notification"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtDescription"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_12pxh"
            android:text="@{emptyNotificationVM.emptyNotificationModel.txtDescription}"
            style="@style/txtAirbnbCerealAppbook16_8" tools:text="@string/msg_lorem_ipsum_dol2"/>
    </LinearLayout>
</layout>
